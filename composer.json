{
    "name": "cakedc/cakephp-datatables",
    "description": "CakePHP datatables plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "CakeDC\\Datatables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakeDC\\Datatables\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    },
    "scripts": {
        "cs-check": "phpcs --colors -p -s --extensions=php src/ tests/"
    }
}
