{
    "name": "CakePHP/cakephp-datatables",
    "description": "Cakephp-datatables plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=7.4",
        "cakephp/cakephp": "4.3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.5 || ^9.3"
    },
    "autoload": {
        "psr-4": {
            "Datatables\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "CakePHP\\Datatables\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}
